URxvt*font: xft:monospace:size=12, xft:Droid Sans Fallback:size=12, xft:Icons, xft:Noto Color Emoji
!URxvt*font: -xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1, -*-unifont-medium-*-*-*-16-*-*-*-*-*-*-*
URxvt*boldFont:
URxvt*italicFont:
URxvt*boldItalicFont:
URxvt*skipBuiltinGlyphs: true
!URxvt*letterSpace: -2

URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollWithBuffer: True
URxvt*scrollTtyKeypress: True
URxvt*saveLines: 65535

URxvt*internalBorder: 0
URxvt*externalBorder: 0
URxvt*geometry: 118x32

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,font-size
URxvt.url-select.launcher: chromium
URxvt.url-select.underline: true
URxvt.keysym.M-C-o: perl:url-select:select_next
URxvt.keysym.M-C-c: perl:clipboard:copy
URxvt.keysym.M-C-v: perl:clipboard:paste
URxvt.keysym.M-C-f: perl:keyboard-select:search
URxvt.keysym.M-C-minus: perl:font-size:decrease
URxvt.keysym.M-C-equal: perl:font-size:increase
URxvt.font-size.step: 1
