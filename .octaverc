% vim: set ft=matlab:
crash_dumps_octave_core(0)
set(0,'defaultAxesXGrid','on');
set(0,'defaultAxesYGrid','on');
set(0,'defaultAxesZGrid','on');
%opengl hardware;

format short g;
format compact;
pkg load signal;
pkg load control;
pkg load symbolic;
pkg load statistics;
%graphics_toolkit('gnuplot'); % Better for printing
graphics_toolkit('qt'); % Better for mouse browsing
more off;
PAGER('less');

pkg load general;
pkg load miscellaneous;
function r = e0
	r = physical_constant('electric constant');
end
function r = mu0
	r = physical_constant('mag. constant');
end
function r = c0
	r = physical_constant('speed of light in vacuum');
end
