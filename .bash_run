#!/bin/bash

. ~/.bashrc

one_command_execute() {
	echo "$READLINE_LINE" >> ~/.bash_history
    eval "$READLINE_LINE" &
    disown
    exit
}

bind -x '"\C-m":"one_command_execute"'

wire() { wireshark; }
mpv() { mpv --player-operation-mode=pseudo-gui; }
vscode() { code; }
transmission() { systemd-inhibit --why='Downloading or uploading torrents' transmission-gtk; }
trans() { transmission; }
minecraft() { java -jar /opt/shiginima/shiginima.jar; }
mc() { minecraft; }
